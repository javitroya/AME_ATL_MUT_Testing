<?xml version="1.0" encoding="ASCII"?>
<CPL:CPL xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:CPL="http://CPL"
    xsi:schemaLocation="http://CPL /ruleTypeExtraction/Example/CPL2SPL/CPL2SPL1Package/CPL.ecore">
  <subActions
      id="voicemail">
    <contents
        xsi:type="CPL:Location"
        url="sip:jones@voicemail.example.com">
      <contents
          xsi:type="CPL:Redirect"/>
    </contents>
  </subActions>
  <incoming>
    <contents
        xsi:type="CPL:Location"
        url="sip:jones@phone.example.com">
      <contents
          xsi:type="CPL:Proxy"
          timeout="8">
        <busy>
          <contents
              xsi:type="CPL:SubCall"
              ref="voicemail"/>
        </busy>
        <noAnswer>
          <contents
              xsi:type="CPL:AddressSwitch"
              field="origin">
            <otherwise>
              <contents
                  xsi:type="CPL:SubCall"
                  ref="voicemail"/>
            </otherwise>
            <addresses
                is="sip:boss@example.com">
              <contents
                  xsi:type="CPL:Location"
                  url="tel:+19175551212">
                <contents
                    xsi:type="CPL:Proxy"/>
              </contents>
            </addresses>
          </contents>
        </noAnswer>
      </contents>
    </contents>
  </incoming>
</CPL:CPL>

<?xml version="1.0" encoding="ASCII"?>
<CPL xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="CPL">
  <subActions
      id="voicemail">
    <contents
        xsi:type="Location"
        url="sip:jones@voicemail.example.com">
      <contents
          xsi:type="Redirect"/>
    </contents>
  </subActions>
  <incoming>
    <contents
        xsi:type="Location"
        url="sip:jones@phone.example.com">
      <contents
          xsi:type="Proxy"
          timeout="8">
        <busy>
          <contents
              xsi:type="SubCall"
              ref="voicemail"/>
        </busy>
        <noAnswer>
          <contents
              xsi:type="AddressSwitch"
              field="origin">
            <otherwise>
              <contents
                  xsi:type="SubCall"
                  ref="voicemail"/>
            </otherwise>
            <addresses
                is="sip:boss@example.com">
              <contents
                  xsi:type="Location"
                  url="tel:+19175551212">
                <contents
                    xsi:type="Proxy"/>
              </contents>
            </addresses>
          </contents>
        </noAnswer>
      </contents>
    </contents>
  </incoming>
</CPL>
